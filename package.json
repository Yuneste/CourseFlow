    "canvas-confetti": "^1.9.3",
    "chalk": "^5.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.9",
    "ioredis": "^5.7.0",
    "lucide-react": "^0.526.0",
    "next": "^14.2.30",
    "next-themes": "^0.4.6",
    "ora": "^8.2.0",
    "react": "^18",
