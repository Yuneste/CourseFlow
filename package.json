{
  "name": "courseflow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest --coverage",
    "db:backup": "tsx scripts/backup-database.ts",
    "db:backup:prod": "cross-env NODE_ENV=production tsx scripts/backup-database.ts",
    "db:migrate": "node scripts/migrate-database.js up",
    "db:rollback": "node scripts/migrate-database.js rollback",
    "db:status": "node scripts/migrate-database.js status",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "prestart": "npm run db:migrate",
    "clean": "node scripts/clean.js",
    "clean:all": "node scripts/clean.js && rm -rf node_modules",
    "test:load:basic": "k6 run tests/load/k6-basic.js",
    "test:load:full": "k6 run tests/load/k6-full-flow.js",
    "test:load:report": "k6 run --out html=tests/load/report.html tests/load/k6-basic.js",
    "deploy:check": "tsx scripts/pre-deployment-checklist.ts",
    "deploy:production": "tsx scripts/deploy-production.ts",
    "deploy:staging": "tsx scripts/deploy-production.ts --staging"
  },
  "dependencies": {
